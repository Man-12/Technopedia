<h1 class="pb-4">Explore Latest</h1>

<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
  <% if(tech != '') {%> <% tech.forEach(function(tech, index){ %>
  <a href="/tech/<%= tech._id %>" class="col text-center category__link">
    <div class="category__img category__img--large shadow">
      <img
        src="/uploads/<%= tech.image %>"
        alt="<%= tech.name %>"
        loading="lazy"
      />
    </div>
    <div class="pt-1"><%= tech.name %></div>
  </a>
  <% }) %> <% } else { %>
  <p>No Items found</p>
  <% } %>
</div>
